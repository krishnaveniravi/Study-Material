#using find()
def substring(string,sub):
  if (string.find(sub))==-1:
    return 'Substring not found'
  else:
    return 'Substring found'

string = "geeks for geeks"
sub_str = "geek"
print(substring(string,sub_str))
---
#using index()
def substring(string,sub):
  try:
    if (string.index(sub))>=0:
      return 'Substring found'
    else:
      return 'Substring not found'
  except:
    return 'Enter valid input'

string = "geeks for geeks"
sub_str = "geek"
print(substring(string,sub_str))
----------
#using lambda
string = "geeks for geeks"
sub_str = "geek"
res=''.join(filter(lambda x:sub_str in string,string))
print('Yes!found' if res else 'Not Found!')
============
def is_substring(string, substring):
    for i in range(len(string) - len(substring) + 1):
        if string[i:i+len(substring)] == substring:
            return True
    return False
string = "A geeks in need is a geek indeed"
substring = "geeks"
print(is_substring(string,substring))
-----------
import re
MyString1 = "A geek in need is a geek indeed"
