#63.Write a Python program to check if a given string is binary string or not.
  def is_binary_string(string):
      for i in string:
          if i not in '01':
              return False
      return True
  
  input_str='011100'
  if is_binary_string(input_str):#True
      print(f'{input_str} is a binary string')
  else:
      print(f'{input_str} is not a binary string')
